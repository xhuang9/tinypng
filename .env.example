# Your TinyPNG API key
TINIFY_API_KEY=your_real_api_key_here

# Optional: absolute path to the folder to process (default: current project folder)
TARGET_FOLDER=null

# File types to include, separated by pipe |
FILE_EXTENSIONS=png|jpg

# Include subfolders recursively?
INCLUDE_SUB_FOLDER=true

# Overwrite original files?
REPLACE_EXISTING_FILE=true

# Resize before compressing?
SHOULD_RESIZE=true

# Resize dimensions
RESIZE_MAX_WIDTH=1920
RESIZE_MAX_HEIGHT=null