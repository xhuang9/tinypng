TINIFY_API_KEY=your_real_api_key_here # Your TinyPNG API key (get one at https://tinypng.com/developers)
INCLUDE_SUB_FOLDER=true # Should subfolders be included? (true/false)
REPLACE_EXISTING_FILE=true # Replace the original files? (true = overwrite, false = save to /minified)
SHOULD_RESIZE=true # Should images be resized before compression?
RESIZE_MAX_WIDTH=1920 # Resize max width in pixels (e.g., 1920)
RESIZE_MAX_HEIGHT=null # Resize max height in pixels (null = auto)